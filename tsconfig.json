{
    "compilerOptions": {
        "target": "ES2021",
        "module": "ES2022",
        "moduleResolution": "node",
        "types": ["bun-types"],
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noImplicitThis": true,
        "skipLibCheck": true
    }
}
